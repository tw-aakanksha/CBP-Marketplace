{
  "prettier/prettier": [
    "error",
    {
      "singleQuote": true,
      "parser": "flow",
      "printWidth": 80,
      "editor.formatOnSave": true,
      "proseWrap": "always",
      "tabWidth": 4,
      "requireConfig": false,
      "useTabs": false,
      "trailingComma": "none",
      "bracketSpacing": true,
      "jsxBracketSameLine": false,
      "semi": true
    }
  ]
}
